{
  "name": "Github Chrome",
  "version": "0.0.1",
  "manifest_version":2,
  "description": "A chrome extension for github",
  "browser_action": {
    "default_title": "Github",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": [
      "libs/underscore-min.js",
      "libs/jquery-2.0.2.min.js",
      "libs/backbone-min.js",
      "build/application.js",
      "build/hook.js"
      ],
    "css": ["assets/css/main.css"]
    }
  ],
  "background": {
    "scripts" : []
  },
  "permissions": ["storage"]
}
